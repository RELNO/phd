@article{Klopp2017,
	title     = {{The urban sustainable development goal: Indicators, complexity and the politics of measuring cities}},
	author    = {Klopp, Jacqueline M. and Petretta, Danielle L.},
	year      = 2017,
	month     = {mar},
	journal   = {Cities},
